import java.util.*;

public class averageofthreenumber.java {
    public static void main(String args[]){
        System.out.print("enter 3 number:");
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int y = sc.nextInt();
        int z= sc.nextInt();
         
       int avg = (x+y+z)/3;
       System.out.print("average is:" +avg);

    }


    
}
